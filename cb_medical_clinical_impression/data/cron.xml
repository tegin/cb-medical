<odoo noupdate="1">
        <record
        id="cron_medical_clinical_impression_validation"
        model="ir.cron"
        forcecreate="True"
    >
        <field name="name">Medical Impression auto validation</field>
        <field name="active" eval="True" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field
            name="model_id"
            ref="medical_clinical_impression.model_medical_clinical_impression"
        />
        <field name="state">code</field>
        <field name="code">model._cron_validate_clinical_impression(5)</field>
    </record>
</odoo>
