<odoo>
    <record model="ir.ui.view" id="medical_product_request_form_view">
        <field
            name="name"
        >medical.product.request.form (in cb_medical_product_request)</field>
        <field name="model">medical.product.request</field>
        <field
            name="inherit_id"
            ref="medical_product_request.medical_product_request_form_view"
        />
        <field name="arch" type="xml">
            <field name="requester_id" position="before">
                <field
                    name="expected_dispensation_date"
                    attrs="{'readonly':[('state', '!=', 'draft')]}"
                />
            </field>
        </field>
    </record>
