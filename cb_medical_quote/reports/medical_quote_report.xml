<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_report_medical_quote" model="ir.actions.report">
        <field name="name">Quote</field>
        <field name="model">medical.quote</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">b_medical_quote.report_medical_quote</field>
        <field name="report_file">cb_medical_quote.report_medical_quote</field>
        <field name="print_report_name">'Presupuesto - %s' % (object.name)</field>
    </record>

    <record id="action_report_medical_quote_patient" model="ir.actions.report">
        <field name="name">Patient copy</field>
        <field name="model">medical.quote</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">cb_medical_quote.report_medical_quote_patient</field>
        <field name="report_file">cb_medical_quote.report_medical_quote_patient</field>
        <field name="print_report_name">'Actividad - %s' % (object.name)</field>
    </record>
</odoo>
