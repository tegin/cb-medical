<odoo>

    <record id="medical_laboratory_event_view_form" model="ir.ui.view">
        <field name="name">medical.laboratory.event.view.form</field>
        <field name="model">medical.laboratory.event</field>
        <field name="inherit_id" ref="medical_clinical_laboratory.medical_laboratory_event_view_form"/>
        <field name="arch" type="xml">
            <field name="performer_id" position="after">
                <field name="coverage_amount"/>
                <field name="private_amount"/>
                <field name="is_sellable_private"/>
                <field name="is_sellable_insurance"/>
            </field>
        </field>
    </record>

</odoo>
