<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="assets_backend"
        name="cb_diagnostic_report_assets_backend"
        inherit_id="web.assets_backend"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/cb_medical_diagnostic_report/static/src/js/diagnostic_report_controller.js"
            />
            <script
                type="text/javascript"
                src="/cb_medical_diagnostic_report/static/src/js/diagnostic_report_renderer.js"
            />
            <script
                type="text/javascript"
                src="/cb_medical_diagnostic_report/static/src/js/diagnostic_report_view.js"
            />
            <link
                rel="stylesheet"
                type="text/scss"
                href="/cb_medical_diagnostic_report/static/src/scss/diagnostic_report_view.scss"
            />
        </xpath>
    </template>
    <template
        id="report_assets_common"
        name="Report Assets (used in backend interface)"
        inherit_id="web.report_assets_common"
    >
        <xpath expr="." position="inside">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/cb_medical_diagnostic_report/static/src/scss/medical_report_layout.scss"
            />
        </xpath>
    </template>
</odoo>
